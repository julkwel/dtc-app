{% extends 'base_front.html.twig' %}

{% block title %}DTC | Edit Student{% endblock %}

{% block body %}
	<main id="main">
		<!-- ======= Breadcrumbs ======= -->
		<section class="breadcrumbs">
			<div class="container">
				<h2>{{ app.user.username }} / Ajouter un contact</h2>
			</div>
		</section><!-- End Breadcrumbs -->
		<section id="counts" class="counts">
			<div class="container" data-item="fade-up">
				{{ form_start(form) }}
					{{ form_widget(form) }}
					<button type="submit" class="btn btn-sm btn-primary text-center m-auto mt-2" style="min-width: 150px;">Enregistrer</button>
				{{ form_end(form) }}
			</div>
		</section>
	</main>
{% endblock %}
